{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/intro",
        "dest": "/index.html"
      },
      {
        "src": "/aboutme",
        "dest": "/index.html"
      },
      {
        "src": "/skills",
        "dest": "/index.html"
      },
      {
        "src": "/projects",
        "dest": "/index.html"
      },
      {
        "src": "/art",
        "dest": "/index.html"
      },
      {
        "src": "/certificate",
        "dest": "/index.html"
      },
      {
        "src": "/contact",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  